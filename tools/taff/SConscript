Import('env idir_bin libmmsinfo libmmsconfig libmmstools libmmsgui')
env.Append(LIBS = ['mmsinfo', 'mmsconfig', 'mmstools', 'mmsgui'])
env.Append(LIBPATH = '../../../lib') 
taff = env.Program('../../../bin/taff', Glob('src/*.cpp'))
Depends(taff, [libmmsinfo, libmmsconfig, libmmstools, libmmsgui])
env.Install(idir_bin, taff)
Export('taff')
